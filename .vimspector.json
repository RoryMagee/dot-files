{
    "configurations": {
        "node": {
            "adapter": "vscode-node",
            "breakpoints": {
                "exception": {
                    "all": "N",
                    "uncaught": "Y"
                }
            },
            "configuration": {
                "request": "launch",
                "protocol": "auto",
                "stopOnEntry": true,
                "console": "integratedTerminal",
                "skipFiles": [
                    "*/<node_internals>/**/*.js",
                    "node_modules/**/*.js"
                ],
                "program": "${PWD}/${filename}"
            }
        },
        "express": {
            "adapter": "vscode-node",
            "breakpoints": {
                "exception": {
                    "all": "N",
                    "uncaught": "Y"
                }
            },
            "configuration": {
                "name": "Attach to express application",
                "type": "node",
                "request": "attach",
                "smartStep": true,
                "skipFiles": [
                    "${workspaceFolder}/node_modules/**/*.js",
                    "<node_internals>/**/*.js"
                ],
                "processId": "${processId}"
            }
        },
        "ts": {
            "adapter": "vscode-node",
            "breakpoints": {
                "exception": {
                    "all": "N",
                    "uncaught": "N"
                }
            },
            "configuration": {
                "request": "launch",
                "protocol": "auto",
                "stopOnEntry": true,
                "console": "integratedTerminal",
                "program": "${PWD}/out/${filename}",
                "cwd": "${PWD}"
            }
        },
        "ts-express": {
            "adapter": "vscode-node",
            "breakpoints": {
                "exception": {
                    "all": "N",
                    "uncaught": "Y"
                }
            },
            "configuration": {
                "request": "attach",
                "console": "integratedTerminal",
                "skipFiles": [
                    "*/<node_internals>/**/*.js",
                    "node_modules/**/*.js"
                ],
                "processId": "${processId}"
            }
        },
        "c#": {
            "adapter": "netcoredbg",
            "breakpoints": {
                "exception": {
                    "all": "N",
                    "uncaught": "Y"
                }
            },
            "configuration": {
                "request": "launch",
                "program": "${PWD}/bin/Debug/net5.0/${dll_location}",
                "args": [],
                "stopAtEntry": true,
                "cwd": "${PWD}"
            }
        },
        "go": {
            "adapter": "vscode-go",
            "configuration": {
                "request": "launch",
                "program": "${fileDirname}",
                "mode": "debug",
                "dlvToolPath": "$HOME/go/bin/dlv"
            }
        },
        "go-test": {
            "adapter": "vscode-go",
            "configuration": {
                "request": "launch",
                "program": "${file}",
                "mode": "test",
                "dlvToolPath": "$HOME/go/bin/dlv",
                "showLog": true
            }
        }
    }
}
